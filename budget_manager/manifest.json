{
    "manifest_version": 2,
    "name": "Budget Manager",
    "version": "1.0",
    "description": "Tracks overall spending",
    "icons": {
        "128": "moneyicon.jpg",
        "48": "moneyicon.jpg",
        "16": "moneyicon.jpg"
    },
    "browser_action": {
        "default_icon": "moneyicon.jpg",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "permissions": [
        "storage",
        "notifications"
    ]
}